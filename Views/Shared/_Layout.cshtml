						<script language="javascript">


							function sizeDown(min) {
								$("*").each(function () {
									//get this object's current font size
									var dCurrSize = parseFloat($(this).css("font-size"), 16);
									//the only parameter of the function is the minimum font size
									//if current size's bigger than minimum size we go and resize
									if (dCurrSize > min) {
										//set font-size to current size - 1
										$(this).css("font-size", (dCurrSize - 1).toString() + "px");
									}
								});
							}



							function sizeUp(iHateMax) {
								//see the documentation of the function above. it's almost the same
								$("*").each(function () {
									var dCurrSize = parseFloat($(this).css("font-size"), 16);

									if (dCurrSize < iHateMax) {
										$(this).css("font-size", (dCurrSize + 1).toString() + "px");
									}
								});
							}
							function sizeRe(resize) {
								//see the documentation of the function above. it's almost the same
								$("*").each(function () {
									$(this).css("font-size", 16 + "px");
								});
							}

