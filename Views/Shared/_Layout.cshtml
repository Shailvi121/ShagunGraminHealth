		<script>
			/*Numeric Validation*/
			$(document).on('keypress', '.numbersOnly', function (e) {
				if (e.which != 8 && e.which != 0 && (e.which < 48 || e.which > 57)) {
					return false;
				}
			});
			/*Only Alphabets Validation*/
			$('.alphabetsOnly').keypress(function (e) {
				if ((e.which > 64 && e.which < 91) || (e.which > 96 && e.which < 123) || (e.which == 32) || (e.which == 8) || (e.which == 0)) {
					return true;
				} else {
					return false;
				}
			});
			$('input,select,textarea').bind("drop", function (e) { e.preventDefault(); return false; });

